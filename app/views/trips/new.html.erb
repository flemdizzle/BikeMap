
<%= form_tag("/trip", method: "get") do %>
   <%= label_tag "Starting Location" %>
   <%= text_field_tag :start %><br>
   <%= label_tag "Destination" %>
   <%= text_field_tag :end %>
   <span class="button">
      <%= submit_tag("Submit", :class => "sub-button") %>
   </span>
   <br>
   <%= check_box_tag(:cbs, "true") %>
   <%= label_tag("Capital Bike Share") %>
<% end %>

